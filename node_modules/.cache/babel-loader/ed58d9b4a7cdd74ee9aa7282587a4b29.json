\"/managerGame/getWalletCoins.json\")\n        .then(result => {\n          this.walletlist = result.data.walletlist;\n        })\n        .catch(err => {\n          this.mAlert(\"Ëé∑Âèñ‰ΩôÈ¢ùÂ§±Ë¥•\");\n        });\n    },\n    mGetCoin(item) {\n      this.mLoading(true);\n      this.$http\n        .get(\"/managerGame/getWalletCoin.json?id=\" + item.gameCompanyId)\n        .then(result => {\n          if (result.code == 0 && result.data > 0) {\n            this.walletlist.forEach(wl => {\n              if (wl.gameCompanyId == item.gameCompanyId) {\n                wl.coin = result.data;\n              }\n            });\n          }\n          this.mLoading(false);\n        })\n        .catch(err => {\n          this.mAlert(\"Ëé∑Âèñ‰ΩôÈ¢ùÂ§±Ë¥•\");\n        });\n    },\n    mSave() {\n      if (this.isDoing) {\n        this.mAlert(\"Â¶ÇÈúÄ‰øÆÊîπÔºåËØ∑ËÅîÁ≥ªÂÆ¢ÊúçÔºÅ\", () => {});\n        return this.mInit();\n      }\n      if (!this.vm.realName) {\n        this.mAlert(\"ÁúüÂÆûÂßìÂêçÂøÖÂ°´ÔºÅ\", () => {\n          this.$refs.realName.focus();\n        });\n        return;\n      }\n      if (!this.vm.mobile) {\n        this.mAlert(\"ÊâãÊú∫Âè∑ÂøÖÂ°´ÔºÅ\", () => {\n          this.$refs.mobile.focus();\n        });\n        return;\n      }\n      if (\n        !/^((1[3,4,5,6,7,8,9][0-9])|(14[5,7])|(17[0,6,7,8])|(19[7]))\\d{8}$/.test(\n          this.vm.mobile\n        )\n      ) {\n        this.mAlert(\"ËØ∑ËæìÂÖ•Ê≠£Á°ÆÁöÑÊâãÊú∫Âè∑Á†ÅÊ†ºÂºè\", () => {\n          this.$refs.mobile.focus();\n        });\n        return;\n      }\n      this.isDoing = true;\n      this.mLoading(true);\n      this.$http.post(\"/memberUser/updateinfo.json\", this.vm).then(result => {\n        this.isDoing = false;\n        this.mLoading(false);\n        if (result.code == 0) {\n          this.mAlert(\"ÊÅ≠ÂñúÔºåËÆæÁΩÆÊàêÂäüÔºÅ\", () => {});\n          this.mInit();\n        } else {\n          this.mAlert(result.message, () => {});\n        }\n      });\n    }\n  }\n};\n</script>\n\n<style